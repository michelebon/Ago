  'name': 'Quota di popolazione ultracentenaria',
  'page_idx': 29,
  'table_rows': [{'block_idx': 255,
    'cells': [{'cell_value': 'Numeratore'},
     {'cell_value': 'Popolazione residente di età 100 anni ed oltre'}],
    'type': 'table_data_row'},
   {'block_idx': 256,
